{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "${REDIS_CONNECTION_STRING}",
    "Hangfire": "Server=${DB_SERVER};Database=${HANGFIRE_DB_NAME};User Id=${DB_USER};Password=${DB_PASSWORD};",
    "DefaultConnection": "Server=${DB_SERVER};Database=${DB_NAME};User Id=${DB_USER};Password=${DB_PASSWORD};"
  },
  "Finbuckle:MultiTenant:Stores:ConfigurationStore": {
    "Defaults": {
      "ConnectionString": "Server=${DB_SERVER};Database=${DB_NAME};User Id=${DB_USER};Password=${DB_PASSWORD};"
    },
    "Tenants": [
      {
        "Id": "${TENANT_ID_1}",
        "Identifier": "${TENANT_IDENTIFIER_1}",
        "Name": "${TENANT_NAME_1}",
        "ConnectionString": "${TENANT_CONNECTION_STRING_1}",
        "FAPAccount": {
          "CampusCode": "${FAP_CAMPUS_CODE_1}",
          "Username": "${FAP_USERNAME_1}",
          "Password": "${FAP_PASSWORD_1}"
        }
      }
    ]
  },
  "Hangfire": {
    "Username": "${HANGFIRE_USERNAME}",
    "Password": "${HANGFIRE_PASSWORD}"
  },
  "Jwt": {
    "Secret": "${JWT_SECRET}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "AccessTokenExpiration": "${ACCESS_TOKEN_EXPIRATION}",
    "RefreshTokenExpiration": "${REFRESH_TOKEN_EXPIRATION}"
  },
  "GoogleSettings": {
    "ClientId": "${GOOGLE_CLIENT_ID}",
    "ClientSecret": "${GOOGLE_CLIENT_SECRET}"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "StudentCareSystem.API"
    }
  },
  "FapClient": {
    "Name": "${FAP_API_NAME}",
    "BaseAddress": "${FAP_API_BaseAddress}",
    "ClientCode": "${FAP_API_ClientCode}",
    "HashCode": "${FAP_API_HashCode}"
  },
  "EmailProxy": {
    "BaseAddress": "${EMAIL_PROXY_BASE_ADDRESS}"
  },
  "AISettings": {
    "BaseAddress": "${AI_BASE_ADDRESS}",
    "Key": "${AI_API_KEY}"
  }, 
  "StringeeSettings": {
    "ClientId": "${STRINGEE_CLIENT_ID}",
    "ClientSecret": "${STRINGEE_CLIENT_ID}",
    "ExpiredTime": 3000000,
    "PhoneNumber": "${STRINGEE_PHONE_NUMBER}"
  },
  "CheckSum": {
    "PrivateKey": "${CHECKSUM_SECRET}",
    "PeriodSeconds": "${CHECKSUM_PERIOD}"
  },
  "Cors": {
    "AllowedOrigins": [
      "${CORS_ALLOWED_ORIGINS}"
    ]
  },
  "OTP": {
    "SecretKey": "${OTP_SecretKey}",  
    "TTLSeconds": "${OTP_TTLSeconds}",
    "MaxAttempts": "${OTP_MaxAttempts}",
    "MaxCounter": "${OTP_MaxCounter}",
    "ChangePassword": {
      "TTLSeconds": "${OTP_ChangePassword_TTLSeconds}",
      "MaxCounter": "${OTP_ChangePassword_MaxCounter}",
      "MaxAttempts": "${OTP_ChangePassword_MaxAttempts  }"
    }
  }
}